{
	"compilerOptions": {
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ES2017"
		],
		"target": "ES2017",
		"module": "ES2015",
		"moduleResolution": "Node",
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"checkJs": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": "./src",
		"noEmit": true,
		"outDir": "./lib",
	},
	"include": [
		"src"
	]
}
